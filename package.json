{
  "name": "nasa-ui",
  "version": "0.3.15",
  "description": "NETS Nasa front-end Component Library, base on antd",
  "homepage": "https://github.com/milolu/nasa-ui#readme",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build-clean": "node scripts/build-clean.js",
    "build": "SET NODE_ENV=production&& npm run build-clean && babel src --out-dir lib --source-maps --extensions .es6,.es,.jsx,.js --copy-files --presets=react-app,stage-0",
    "doc": "node scripts/doc.js",
    "push": "npm run build && npm publish",
    "test": "node scripts/test.js --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milolu/nasa-ui.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/milolu/nasa-ui/issues"
  },
  "files": [
    "lib",
    "src"
  ],
  "dependencies": {
    "antd": "^3.0.0",
    "classnames": "^2.2.5",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "marked": "^0.4.0",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.1",
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.9.0",
    "@mdx-js/mdx": "^0.9.0",
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "eslint": "4.10.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "immutability-helper": "^2.6.6",
    "mockjs": "^1.0.1-beta3",
    "npm": "^6.1.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "qs": "^6.5.2",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "sass-loader": "^6.0.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2"
  },
  "proxy": {
    "/api|/auth|/static": {
      "target": "http://127.0.0.1:8200",
